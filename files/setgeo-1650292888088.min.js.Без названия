.search-line-block:visible");if($('[name="fromidcity[]"]:eq(0) option')[0].text!=$('[name="fromidcity[]"]:eq(0) :selected').text()){var i="";i=n.find('[name="fromidc[]"]:eq(0) :selected').text()!=n.find('[name="fromidr[]"]:eq(0) :selected').text()?n.find('[name="fromidcity[]"]:eq(0) :selected').text()+","+n.find('[name="fromidr[]"]:eq(0) :selected').text()+","+n.find('[name="fromidr[]"]:eq(0) :selected').text():n.find('[name="fromidcity[]"]:eq(0) :selected').text()+","+n.find('[name="fromidr[]"]:eq(0) :selected').text(),n.find(".js-placeholder-value-from").val(i)}if($('[name="toidcity[]"]:eq(0) option')[0].text!=$('[name="toidcity[]"]:eq(0) :selected').text()){i="";i=n.find('[name="toidc[]"]:eq(0) :selected').text()!=n.find('[name="toidr[]"]:eq(0) :selected').text()?n.find('[name="toidcity[]"]:eq(0) :selected').text()+","+n.find('[name="toidr[]"]:eq(0) :selected').text()+","+n.find('[name="toidr[]"]:eq(0) :selected').text():n.find('[name="toidcity[]"]:eq(0) :selected').text()+","+n.find('[name="toidr[]"]:eq(0) :selected').text(),n.find(".js-placeholder-value-to").val(i)}}function loadregionspref(e,t,n,o,s,a){o||(o=null);var r="",l=$(t).data("typeForm"),d=$("."+l+" #"+n+"idr select")[0],c=new JsHttpRequest;c.onreadystatechange=function(){if(4==c.readyState&&c.responseJS){d.length=c.responseJS.ids.length;var e=$("."+l+" #"+n+"idsquare")[0];for(i=0;i<c.responseJS.names.length;i++)d[i].text=c.responseJS.names[i],d[i].value=c.responseJS.ids[i],1==c.responseJS.names.length&&0==c.responseJS.ids[i]&&(e.length=c.responseJS.ids.length,e[i].value=c.responseJS.ids[i]);var s="."+n+"-block";if(c.responseJS.square&&c.responseJS.square.names){if(e=$("."+l+" #"+n+"idsquare")[0]){for(e.length>1&&$(e).children("option:not(:first)").remove(),$(e.closest(".square-wrap")).show(),e.length=c.responseJS.square.ids.length,i=0;i<c.responseJS.square.names.length;i++)e[i].text=c.responseJS.square.names[i],e[i].value=c.responseJS.square.ids[i];r=0,$(s+" .require-square-city").hide()}}else e&&($(