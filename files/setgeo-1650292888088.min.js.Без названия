function autoMailOnOff(e){let t=$(e).closest(".result-item__wrap"),a=t.data("itemid"),s=t.data("itemtype"),i=$(e).parent();i.hasClass("disabled")||(i.hasClass("off")?uri="/cabinet/subscribe.html":uri="/cabinet/unsubscribe.html",$.ajax({type:"POST",url:uri,dataType:"JSON",data:"supply_id="+a+"&supply_type="+s,success:function(e){e.success?("subscribe"===e.data.action&&i.removeClass("off").addClass("on"),"unsubscribe"===e.data.action&&i.removeClass("on").addClass("off")):4===e.data.code?alert("Произошла ошибка: отсутствует тарифный план."):alert("Произошла ошибка.")},error:function(){alert("Произошла ошибка.")}}))}$(document).ready(function(){$("#btnSendMarkEmail").on("click",function(e){e.preventDefault(),openPopup(".overlay_sendMarkEmail")}),$(".popup_SendMarkEmail.popup .close").on("click",function(){let e=$("#emailMessage");$("#sendMarkEmail_notification").val("").css({"line-height":"36px",visibility:"hidden",height:"36px"}),e.val("")}),$("#sendMessage").on("click",function(e){e.preventDefault();let t=$(this).data("url");const a="Произошла ошибка отправки сообщений.";let s="Сообщение было успешно доставлено.";$(".userMarkId").length>1&&(s="Сообщения были успешно доставлены.");let i={1:"Пользователь был не найден.",2:"Пользователи были не найдены",3:"Достигнут лимит отправки e-mail сообщений.",4:"Сообщение не доставлено",7:"Произошли ошибки с отправкой сообщений",8:a,10:s};let n=$("#sendMarkEmail_notification"),l=$("#formMarkEmail"),c=$("#emailMessage"),o=$("#sendMessage"),r=l.data("type"),u=[];$("input.userMarkId ").each(function(e,t){u.push("ids[]="+$(t).val())});let d=(u=u.join("&"))+"&dataType="+r+"&message="+c.val();c.val().length>5?$.ajax({url:t,type:"POST",dataType:"json",data:d,beforeSend:function(){o.text("Отправляется...").css("pointer-events","none"),n.html("")},success:function(e){!0===e.success?n.css("color","green"):n.css("color","red"),n.html(i[e.data.code]),7===e.data.code&&(e.data.userNames.forEach(function(e){n.append('<br>Пользователю "'+e+'" сообщение не доставилось.')}),n.css({"line-height":"18px",height:"auto"})),e.success||e.data.code||n.html(a),n.css("visibility","visible")},error:function(){n.val(a).css("color","red")},complete:function(){c.val("").css("visibility","visible"),o.text("Отправить сообщение").css("pointer-events","auto")}}):alert("Заполните текст сообщения")}),$(".notify").after().click(function(e){e.preventDefault(),$(this).slideToggle(200)})}),$(document).ready(function(){$(".cargo_block__notify_btn-close").on("click",function(){$(this).parent().hide(600)})});